{
  "name": "WiFi Based Logger",
  "productName": "Wi-Fi-Based-Logger",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "concurrently \"json-server --watch newData.json --port 4000 \"  \"electron-forge start\"",
    "watch": "nodemon --exec electron .",
    "start-server": "json-server --watch dbs.json --port 4000",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Suraj Saroj",
    "email": "suraj.saroj@humancloud.co.in"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "src/logo.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "LDS"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],  
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "devContentSecurityPolicy": "default-src * self blob: data: gap:; style-src * self 'unsafe-inline' blob: data: gap:; script-src * 'self' 'unsafe-eval' 'unsafe-inline' blob: data: gap:; object-src * 'self' blob: data: gap:; img-src * self 'unsafe-inline' blob: data: gap:; connect-src self * 'unsafe-inline' blob: data: gap:; frame-src * self blob: data: gap:;",
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.js",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/preload.js"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-react": "^7.18.6",
    "@electron-forge/cli": "^6.0.0-beta.65",
    "@electron-forge/maker-deb": "^6.0.0-beta.65",
    "@electron-forge/maker-rpm": "^6.0.0-beta.65",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.65",
    "@electron-forge/maker-zip": "^6.0.0-beta.65",
    "@electron-forge/plugin-webpack": "6.0.0-beta.65",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "axios": "^0.27.2",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.0.0",
    "electron": "^20.1.3",
    "file-saver": "^2.0.5",
    "install": "^0.13.0",
    "node-loader": "^2.0.0",
    "npm": "^8.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^3.0.0",
    "xlsx": "^0.18.5"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.4",
    "@mui/styled-engine-sc": "^5.10.3",
    "@reduxjs/toolkit": "^1.8.5",
    "canvasjs-react-charts": "^1.0.5",
    "concurrently": "^7.4.0",
    "electron-squirrel-startup": "^1.0.0",
    "exceljs": "^4.3.0",
    "express": "^4.18.1",
    "file-loader": "^6.2.0",
    "json-server": "^0.17.0",
    "prop-types": "^15.8.1",
    "react-csv": "^2.2.2",
    "react-redux": "^8.0.2",
    "recharts": "^2.1.14",
    "styled-components": "^5.3.5",
    "url-loader": "^4.1.1",
    "wifi-name": "^3.1.1"
  }
}
